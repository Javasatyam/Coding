package demo_pratices;

public class Test {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
       String s="programmer am i Good";
       String[] xyz=s.split(" ");
      
       String LargeString=" ";
	   
      for(int i=0; i<xyz.length; i++)
      {
    	  int a=xyz[i].length();
    	  
    	  for(int j=1; j<xyz.length; j++)
    	  {
    		  int x=xyz[j].length();
    		  //System.out.println("X value "+x);
    		 
    		  if(a<x)
    		  {
    			  LargeString=xyz[j];
    		  }
    	  }
      }
      System.out.print(LargeString);
	}

}
