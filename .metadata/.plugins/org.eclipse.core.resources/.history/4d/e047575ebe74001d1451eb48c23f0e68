package controle_statment;

public class Loop {
	int i;
	int x;
	int y;
//	void sk(int a,int b)
//	{
//		i=a;
//		x=b;
//	}
    Loop(int a,int b,int c)// Default Constructor. ...
          //Parameterized Constructo
    {
    	i=a;
    	x=b;
    	y=c;
    }
	
	

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
    Loop ss=new Loop(1,2,3);
    System.out.println(ss.x);
    
   

}
}