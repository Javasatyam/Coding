package interview;

public class Test {

	public static void main(String[] args) {
		
		// TODO Auto-generated method stub
      String xyz="Th is java programm";
      String abc="is";
      String [] arr=xyz.split("\\s");
      int count=0;
      for(int i=0; i<arr.length; i++)
      {
    	  if(arr[i].contains(abc))
    	  {
    		  count++;
    	  }
      }
      System.out.println(count);
}
}
